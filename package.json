{
  "name": "backend-coding-test",
  "version": "1.0.0",
  "description": "A coding assessment for Xendit backend interviews",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "dev": "nodemon --watch 'src/**/*' --watch 'index.ts' -e ts,tsx --exec ts-node index.ts",
    "build-ts": "tsc",
    "tslint": "tslint -c tslint.json -p tsconfig.json -t verbose",
    "tslint-fix": "tslint --fix -c tslint.json -p tsconfig.json -t verbose",
    "build": "npm run tslint && npm run build-ts",
    "test": "mocha tests",
    "start": "npm run build && node dist/index.js"
  },
  "keywords": [
    "Xendit"
  ],
  "author": "Bo",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.16.4",
    "sqlite3": "^4.0.6"
  },
  "devDependencies": {
    "mocha": "^6.1.4",
    "nodemon": "^2.0.12",
    "supertest": "^4.0.2",
    "ts-node": "^10.2.0",
    "typescript": "^4.3.5"
  }
}
